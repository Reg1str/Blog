@model IEnumerable<Post>

@{
    
}
<h2 style="color: gray; margin-left: 2rem">Admin Panel!</h2>
<a asp-controller="Panel" asp-action="Edit" style="margin-left: 2rem">
    Add New Post
</a>
<div>
    <ul>
        @foreach (var post in Model)
        {
            <li style="color: gray">
                @post.Title
                -
                <a asp-controller="Panel" asp-action="Edit" asp-route-id="@post.Id">
                    Edit
                </a>
                -
                <a asp-controller="Panel" asp-action="Remove" asp-route-id="@post.Id">
                    Remove
                </a>
            </li>
        }
    </ul>
</div>